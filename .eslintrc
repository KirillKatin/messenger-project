{
  "env": {
    "es2021": true,
    "node": true,
    "browser": false
  },
  "globals": {
    "process": true,
    "__dirname": true,
    "__filename": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-process-env": 0,
    "no-unused-vars": ["error", { "varsIgnorePattern": "^__" }],
    "no-underscore-dangle": ["error", { "allow": ["__filename", "__dirname"] }]
  }
}
